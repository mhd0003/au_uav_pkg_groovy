<?xml version="1.0" ?>
<mavlink>
        <version>1</version>
    <include>common.xml</include>
        <enums></enums>
        <messages>
            <message id="199" name="AU_REU_UAV">
                <description>The telemrety message for the Auburn REU program.  </description>
                <field type="int32_t" name="au_lat">Target waypoint latitude expressed as 1E7</field>
                <field type="int32_t" name="au_lon">Target waypoint longitude expressed     as 1E7</field>
                <field type="int32_t" name="au_alt">Target waypoint altitude expressed as 1000 (millimeters)</field>
                <field type="int32_t" name="au_ground_speed">Ground speed in cm/sec</field>
                <field type="int32_t" name="au_target_bearing">Target bearing torward next waypoint in degress (0 to 360)</field>
                <field type="int32_t" name="au_distance">Distance to next waypoint expressed in meters</field>
            </message>
        </messages>
</mavlink>